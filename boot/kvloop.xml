<config>
    <kernel args="kernel -f $fs.path" />
    <dom>
        <app args="root">
            <dom>
                <app args="net -m 2 -d default net 192.168.69.2 " daemon="1">
                    <serv name="net" />
                    <tiles type="nicdev" />
                </app>
            </dom>
            <dom>
                <app args="smoltcp_server" daemon="1">
                    <serv name="smoltcp_server" />
                </app>
            </dom>
            <dom>
                <app args="smoltcp_client tcp 192.168.69.2 6969" daemon="0">
                    <sess name="net" args="bufs=1M socks=1" />
                    <sem name="net" />
                </app>
            </dom>
        </app>
    </dom>
</config>
